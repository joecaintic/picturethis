<script src="/bower_components/polymer/webcomponentsjs/webcomponents.js"></script>
<link rel="import" href="/bower_components/x-meme/x-meme.html">
<li class="<%= self_or_other(message) %>">
  <div class="avatar">
    <img src="http://placehold.it/50x50" />
  </div>
  <div class="chatboxmessagecontent">
    <x-meme src="<%= message.image %>" top="<%= message.body %>" bottom="<%= message.body2 %>"></x-meme>
    <time datatime="<%= message.created_at %>" title="<%= message.created_at.strftime("%d %b %Y at %I:%M%p") %>">
      <%= message_interlocutor(message).name %> â€¢ <%= message.created_at.strftime("%H:%M %p") %>
    </time>
  </div>
</li>
